mk_alias em 'emacs -nw'
mk_alias emc 'emacsclient -nw --alternate-editor='''
mk_alias mv 'mv -i'
mk_alias cp 'cp -i'
mk_alias rm 'rm -i'
mk_alias please sudo

mk_alias c ccat
mk_alias ls exa ls
